{
  "version": "0.2.0",
  "configurations":[
    {
      "name": "WebDemo Debug x64",
      "type": "cppdbg",
      "targetArchitecture": "x64",
      "request": "launch",
      "program": "${workspaceRoot}/out/Debug/webp_demo",
      "args": [
